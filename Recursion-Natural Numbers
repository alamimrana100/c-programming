#include <stdio.h>
void printNatural(int current, int n) {
if (current > n)
return;
printf("%d", current);
if (current != n)
printf(" ");
printNatural(current + 1, n);
}
int main() {
int num;
scanf("%d", &num);
if (num >= 1 && num <= 15) {
 printf("The first %d Natural Numbers are:\n", num);
printNatural(1, num);
printf("\n");  
} else {
printf("Enter a Valid Input!!!!!!!!!!!!!!!!!!!!!!!!\n");
 }
 return 0;
}
