#include <stdio.h>
int main() {
int n;
scanf("%d", &n);
if (n <= 0) {
printf("Invalid Input");
return 0;
}
int ages[n];
int i, youngest, oldest;
for (i = 0; i < n; i++) {
scanf("%d", &ages[i]);
if (ages[i] < 0) {
printf("Invalid Input");
return 0;
}
}
youngest = ages[0];
oldest = ages[0];
for (i = 1; i < n; i++) {
if (ages[i] < youngest) {
youngest = ages[i];
}
if (ages[i] > oldest) {
oldest = ages[i];
}
}
 printf("Youngest=%d\n", youngest);
 printf("Oldest=%d\n", oldest);
 return 0;
}
